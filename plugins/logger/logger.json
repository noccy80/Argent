{
	"destinations":{
		"text_log":{
			"type":"text",
			"filters":[
				{"type":"error", "path":"./log/error.log"}
			],
			"default_path":"./log/access.log",
			"timestamp_format":"dMY-H:i:s.u",
			"separator":" | "
		},
		"stdout":{
			"type":"console",
			"filters":[
				{"type":"error", "path":"stderr"}
			],
			"default_path":"stdout",
			"timestamp_format":"dMY-H:i:s.u",
			"separator":" | "
		},
		"html_log":{
			"type":"html",
			"filters":[
				{"type":"error", "path":"./log/error.html"}
			],
			"default_path":"./log/access.html",
			"timestamp_format":"dMY-H:i:s.u"
		}
	},
	"default_destination":"stdout",
	"default_timestamp_format":"dMY-h:m:s.u",
	"default_separator":" | ",
	"default_filter_type":"info"
}
